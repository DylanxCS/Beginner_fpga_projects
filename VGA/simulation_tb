#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 11;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5cc072c24310 .scope module, "Sync_Pulse_tb" "Sync_Pulse_tb" 2 2;
 .timescale -9 -11;
v0x5cc072c3b280_0 .net "H_Pulse", 0 0, L_0x5cc072c12990;  1 drivers
v0x5cc072c3b320_0 .net "V_Pulse", 0 0, L_0x5cc072c12df0;  1 drivers
v0x5cc072c3b3c0_0 .var "h_sync_triggered", 0 0;
v0x5cc072c3b460_0 .var "r_Clock", 0 0;
v0x5cc072c3b500_0 .var "v_sync_triggered", 0 0;
S_0x5cc072c244a0 .scope module, "inst" "Sync_Pulse" 2 7, 3 1 0, S_0x5cc072c24310;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "CLK";
    .port_info 1 /OUTPUT 1 "H_pulse";
    .port_info 2 /OUTPUT 1 "V_pulse";
L_0x5cc072c12990 .functor BUFZ 1, v0x5cc072c3b080_0, C4<0>, C4<0>, C4<0>;
L_0x5cc072c12df0 .functor BUFZ 1, v0x5cc072c3b140_0, C4<0>, C4<0>, C4<0>;
v0x5cc072c12b30_0 .net "CLK", 0 0, v0x5cc072c3b460_0;  1 drivers
v0x5cc072c3ad10_0 .net "H_pulse", 0 0, L_0x5cc072c12990;  alias, 1 drivers
v0x5cc072c3add0_0 .net "V_pulse", 0 0, L_0x5cc072c12df0;  alias, 1 drivers
v0x5cc072c3ae70_0 .var "r_CountCol", 14 0;
v0x5cc072c3af50_0 .var "r_CountRow", 15 0;
v0x5cc072c3b080_0 .var "r_H_pulse", 0 0;
v0x5cc072c3b140_0 .var "r_V_pulse", 0 0;
E_0x5cc072be8dc0 .event posedge, v0x5cc072c12b30_0;
    .scope S_0x5cc072c244a0;
T_0 ;
    %pushi/vec4 0, 0, 15;
    %store/vec4 v0x5cc072c3ae70_0, 0, 15;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x5cc072c3af50_0, 0, 16;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cc072c3b080_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cc072c3b140_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x5cc072c244a0;
T_1 ;
    %wait E_0x5cc072be8dc0;
    %load/vec4 v0x5cc072c3ae70_0;
    %pad/u 32;
    %cmpi/u 25000, 0, 32;
    %jmp/0xz  T_1.0, 5;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5cc072c3b080_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5cc072c3ae70_0;
    %pad/u 32;
    %cmpi/u 31250, 0, 32;
    %jmp/0xz  T_1.2, 5;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5cc072c3b080_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 0, 0, 15;
    %assign/vec4 v0x5cc072c3ae70_0, 0;
T_1.3 ;
T_1.1 ;
    %load/vec4 v0x5cc072c3ae70_0;
    %addi 1, 0, 15;
    %assign/vec4 v0x5cc072c3ae70_0, 0;
    %load/vec4 v0x5cc072c3af50_0;
    %pad/u 32;
    %cmpi/u 47619, 0, 32;
    %jmp/0xz  T_1.4, 5;
    %load/vec4 v0x5cc072c3af50_0;
    %addi 1, 0, 16;
    %assign/vec4 v0x5cc072c3af50_0, 0;
T_1.4 ;
    %load/vec4 v0x5cc072c3af50_0;
    %pad/u 32;
    %cmpi/u 43537, 0, 32;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %jmp/0xz  T_1.6, 5;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5cc072c3b140_0, 0;
    %jmp T_1.7;
T_1.6 ;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x5cc072c3af50_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5cc072c3b140_0, 0;
T_1.7 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5cc072c24310;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cc072c3b460_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cc072c3b3c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cc072c3b500_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x5cc072c24310;
T_3 ;
    %delay 2000, 0;
    %load/vec4 v0x5cc072c3b460_0;
    %inv;
    %assign/vec4 v0x5cc072c3b460_0, 0;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5cc072c24310;
T_4 ;
    %wait E_0x5cc072be8dc0;
    %load/vec4 v0x5cc072c3b280_0;
    %cmpi/e 0, 0, 1;
    %flag_get/vec4 4;
    %jmp/0 T_4.2, 4;
    %load/vec4 v0x5cc072c3b3c0_0;
    %nor/r;
    %and;
T_4.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.0, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5cc072c3b3c0_0, 0;
    %vpi_call 2 21 "$display", "H_sync went to 1'b0 at time %0t", $time {0 0 0};
T_4.0 ;
    %load/vec4 v0x5cc072c3b320_0;
    %cmpi/e 0, 0, 1;
    %flag_get/vec4 4;
    %jmp/0 T_4.5, 4;
    %load/vec4 v0x5cc072c3b500_0;
    %nor/r;
    %and;
T_4.5;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.3, 8;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5cc072c3b500_0, 0;
    %vpi_call 2 26 "$display", "V_sync went to 1'b0 at time %0t", $time {0 0 0};
T_4.3 ;
    %jmp T_4;
    .thread T_4;
    .scope S_0x5cc072c24310;
T_5 ;
    %vpi_call 2 36 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 37 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5cc072c244a0 {0 0 0};
    %delay 220000000, 0;
    %vpi_call 2 40 "$finish" {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "Sync_Pulse_tb.v";
    "Sync_Pulse.v";
